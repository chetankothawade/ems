{
  "require": {
    "php": "^8.2",
    "slim/slim": "^4.13",
    "nyholm/psr7": "^1.8",
    "nyholm/psr7-server": "^1.1",
    "php-di/php-di": "^7.1",
    "doctrine/orm": "^3.6",
    "doctrine/dbal": "^4.4",
    "doctrine/migrations": "^3.9",
    "symfony/cache": "^7.4",
    "symfony/console": "^7.4",
    "ramsey/uuid": "^4.9",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^11"
  },
  "scripts": {
    "test": "phpunit",
    "test:unit": "phpunit tests/Unit",
    "test:int": "phpunit tests/Integration"
  }
}